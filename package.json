{
  "name": "redux-perf-middleware",
  "version": "1.2.2",
  "description": "Measure the time that actions needs to change the state",
  "main": "index.js",
  "scripts": {
    "test": "npm run compile && mocha --compilers js:babel-register --require babel-polyfill",
    "compile": "babel --presets es2015,stage-0 --require babel-polyfill -d lib/ src/",
    "prepublish": "npm run compile",
    "watch": "npm-scripts-watcher"
  },
  "watch": {
    "src/**/*.js": [
      "compile"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AvraamMavridis/redux-perf-middleware.git"
  },
  "keywords": [
    "redux",
    "logger",
    "redux-logger",
    "redux",
    "middleware",
    "redux-perf-logger",
    "redux-perf-middleware",
    "redux-performance",
    "performance"
  ],
  "author": "Avraam Mavridis <avraammauridis@gmail.com> (http://avraammavridis.github.io/)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-plugin-transform-decorators": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "eslint-config-google": "^0.3.0",
    "eslint-plugin-react": "^3.11.2",
    "mocha": "^3.0.0",
    "promise": "^7.1.1",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0"
  },
  "bugs": {
    "url": "https://github.com/AvraamMavridis/redux-perf-middleware/issues"
  },
  "homepage": "https://github.com/AvraamMavridis/redux-perf-middleware#readme",
  "dependencies": {
    "present": "^1.0.0"
  }
}
